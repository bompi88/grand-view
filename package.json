  "scripts": {
    "dev": "node .scripts/run",
    "reset": "meteor reset",
    "build:osx": "node .scripts/download --platform=darwin --arch=x64 && node .scripts/prepare --platform=darwin --arch=x64 && build --mac --config ./packager.json --publish never",
    "build:osx:verbose": "node .scripts/download --platform=darwin --arch=x64 && METEOR_DEBUG_BUILD=1 node .scripts/prepare --platform=darwin --arch=x64 && DEBUG=electron-builder build --mac --config ./packager.json --publish never",
    "build:linux:x86": "node .scripts/download --platform=linux --arch=x86 && node .scripts/prepare --platform=linux --arch=x86 && build --linux --ia32 --config ./packager.json --publish never",
    "build:linux:x64": "node .scripts/download --platform=linux --arch=x64 && node .scripts/prepare --platform=linux --arch=x64 && build --linux --x64 --config ./packager.json --publish never",
    "build:win:x86": "node .scripts/download --platform=win32 --arch=x86 && node .scripts/prepare --platform=win32 --arch=x86 && build --win --ia32 --config ./packager.json --publish never",
    "build:win:x64": "node .scripts/download --platform=win32 --arch=x64 && node .scripts/prepare --platform=win32 --arch=x64 && build --win --x64 --config ./packager.json --publish never",
    "lint": "eslint ./lib ./client ./server --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha meteor/client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx"
  },
  "private": true
}
